<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zeropoint.homemaking.DAO.BannersMapper">
  <resultMap id="BaseResultMap" type="com.zeropoint.homemaking.Entitys.Banners">
    <result column="banner_id" jdbcType="INTEGER" property="bannerId" />
    <result column="banner" jdbcType="VARCHAR" property="banner" />
    <result column="banner_address" jdbcType="VARCHAR" property="bannerAddress" />
  </resultMap>
  <insert id="insert" parameterType="com.zeropoint.homemaking.Entitys.Banners">
    insert into banners (banner_id, banner, banner_address
      )
    values (#{bannerId,jdbcType=INTEGER}, #{banner,jdbcType=VARCHAR}, #{bannerAddress,jdbcType=VARCHAR}
      )
  </insert>
  <select id="selectAll" resultMap="BaseResultMap">
    select banner_id, banner, banner_address
    from banners
  </select>
</mapper>