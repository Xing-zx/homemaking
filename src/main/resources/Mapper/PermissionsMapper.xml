<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zeropoint.homemaking.DAO.PermissionsMapper">
  <resultMap id="BaseResultMap" type="com.zeropoint.homemaking.Entitys.Permissions">
    <result column="sions_id" jdbcType="INTEGER" property="sionsId" />
    <result column="sions_name" jdbcType="VARCHAR" property="sionsName" />
  </resultMap>
  <insert id="insert" parameterType="com.zeropoint.homemaking.Entitys.Permissions">
    insert into permissions (sions_id, sions_name)
    values (#{sionsId,jdbcType=INTEGER}, #{sionsName,jdbcType=VARCHAR})
  </insert>
  <select id="selectAll" resultMap="BaseResultMap">
    select sions_id, sions_name
    from permissions
  </select>
</mapper>